package main

import "fmt"

func main() {
	buf := make([]int, 0, 10)

	fmt.Printf("buf cap:%d | len:%d\n", cap(buf), len(buf))

	buf = append(buf, 1)
	fmt.Printf("buf cap:%d | len:%d\n", cap(buf), len(buf))

	buf = buf[:0]
	fmt.Printf("buf cap:%d | len:%d\n", cap(buf), len(buf))

	var buff []int
	fmt.Printf("buff cap:%d | len:%d\n", cap(buff), len(buff))

}
